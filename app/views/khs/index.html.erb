<p id="notice"><%= notice %></p>

<h1>Khs</h1>

<table>
  <thead>
    <tr>
      <th>NIM </td>
      <th>Kode MK </th>
      <th>Nilai </th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @khs.each do |kh| %>
      <tr>
        <td><%= kh.mahasiswa.nim %> </td>
        <td><%= kh.mata_kuliah.kode_mk %> </td>
        <td><%= kh.nilai %>
        <td><%= link_to 'Show', kh %></td>
        <td><%= link_to 'Edit', edit_kh_path(kh) %></td>
        <td><%= link_to 'Destroy', kh, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

